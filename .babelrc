{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx"],
        "alias": {
          "util": "./util",
          "common-util": "./common-util",
          "context": ["./context"],
          "components": "./components",
          "images": "./public/images",
          "store": "./store"
        }
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "import",
      { "libraryName": "antd", "libraryDirectory": "lib", "style": true }
    ]
  ]
}
